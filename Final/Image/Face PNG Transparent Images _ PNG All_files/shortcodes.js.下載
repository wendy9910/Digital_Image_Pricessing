jQuery(document).ready(function(){jQuery("#tabs").tabs({fx:{opacity:'show'}});jQuery(".tabs").tabs({fx:{opacity:'show'}});jQuery(".toggle").each(function(){if(jQuery(this).attr('data-id')=='closed'){jQuery(this).accordion({header:'h4',collapsible:!0,active:!1})}else{jQuery(this).accordion({header:'h4',collapsible:!0})}})})